{
  "name": "gatsby-source-setizer",
  "description": "Manage source from Google Sheets generated by the SetizerCMS.",
  "version": "0.0.1",
  "author": "Alessandro Aloisio <alessaloisio@gmail.com>",
  "dependencies": {
    "axios": "^0.19.2",
    "lodash": "^4.17.19",
    "pluralize": "^8.0.0"
  },
  "keywords": [
    "gatsby",
    "gatsby-source",
    "gatsby-plugin"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/setizer/gatsby-source-setizer.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/runtime": "^7.4.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-gatsby-package": "^0.1.4",
    "cross-env": "^5.2.0",
    "install-peers": "^1.0.3"
  },
  "license": "MIT",
  "scripts": {
    "build": "babel src --out-dir .",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir ."
  },
  "peerDependencies": {
    "gatsby": ">2.0.15"
  }
}
